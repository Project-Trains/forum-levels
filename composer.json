    "keywords": [
        "extension",
        "flarum",
        "experience",
        "level",
        "xp"
    ],
    "type": "flarum-extension",
    "authors": [
        {
            "name": "kalucky0",
            "email": "kalucki23@gmail.com",
            "homepage": "https://kalucky0.dev/"
        },
        {
            "name": "Shahiem Seymor",
            "email": "s.seymor@live.nl",
            "homepage": "http://shahiemseymor.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/Project-Trains/forum-levels/issues",
        "source": "https://github.com/Project-Trains/forum-levels"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "extra": {
        "flarum-extension": {
            "title": "Forum Levels",
            "icon": {
                "name": "fas fa-percent",
                "backgroundColor": "#263238",
                "color": "#fff"
